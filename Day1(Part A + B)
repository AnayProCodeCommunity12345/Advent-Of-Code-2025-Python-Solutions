import sys
puzzle = sys.stdin.read()
pos = 50
p1 = 0
p2 = 0

for line in puzzle.splitlines():
    a = line[0]
    b = int(line[1:])
    
    for x in range(b):
        if a =='L':
           pos = (pos+99) % 100 #pos-1 + 100
        else:
           pos = (pos+1) % 100
           
        if pos == 0:
           p2 += 1
           
    if pos==0:
       p1 += 1
       
print(p1)
print(p2) 
