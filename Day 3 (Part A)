import sys
puzzle = sys.stdin.read().splitlines()
current_sum = 0


for char in puzzle:
  list1 = []
  for i in range(0, len(char)):
    for k in range(i+1, len(char)):
      list1.append(str(char[i]) + str(char[k]))
      
    if i == len(char) - 1:
      current_sum += max(list(map(int, list1)))
      
print(current_sum)
